{
    "name": "fold-staking",
    "description": "Uniswap V3 staking for FOLD",
    "license": "GPL-3.0-or-later",
    "version": "1.0.0",
    "homepage": "https://www.mevauction.com/Relay/captive-insurance/",
    "keywords": [
        "manifold",
        "mev",
        "FOLD",
        "captive insurance"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/manifoldfinance/fold-staking"
    },
    "files": [
        "contracts",
        "!contracts/test",
        "artifacts/contracts/**/*.json",
        "!artifacts/contracts/**/*.dbg.json",
        "!artifacts/contracts/test/**/*",
        "!artifacts/contracts/base/**/*"
    ],
    "engines": {
        "node": ">=10"
    },
    "dependencies": {
        "@openzeppelin/contracts": "^4.9.6",
        "bn.js": "^5.2.1"
    },
    "devDependencies": {
        "@nomicfoundation/hardhat-chai-matchers": "^2.0.6",
        "@nomicfoundation/hardhat-ethers": "^3.0.5",
        "@nomicfoundation/hardhat-ignition": "^0.15.2",
        "@nomicfoundation/hardhat-ignition-ethers": "^0.15.2",
        "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
        "@nomicfoundation/hardhat-toolbox": "^5.0.0",
        "@nomicfoundation/hardhat-verify": "^2.0.6",
        "@nomicfoundation/ignition-core": "^0.15.2",
        "@nomiclabs/hardhat-ethers": "^2.0.2",
        "@typechain/ethers-v5": "^7.0.0",
        "@typechain/ethers-v6": "^0.5.1",
        "@typechain/hardhat": "^2.3.1",
        "@types/chai": "^4.2.6",
        "@types/console-log-level": "^1.4.0",
        "@types/lodash": "^4.14.170",
        "@types/mocha": "^5.2.7",
        "@typescript-eslint/eslint-plugin": "^4.26.0",
        "@typescript-eslint/parser": "^4.26.0",
        "chai": "^4.2.0",
        "console-log-level": "^1.4.1",
        "dotenv": "^16.4.5",
        "eslint": "^7.28.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-unused-imports": "^1.1.1",
        "ethers": "^6.13.0",
        "hardhat": "^2.22.3",
        "hardhat-contract-sizer": "^2.10.0",
        "hardhat-gas-reporter": "^1.0.10",
        "lodash": "^4.17.21",
        "mocha-chai-jest-snapshot": "^1.1.0",
        "prettier": "^2.2.1",
        "prettier-check": "^2.0.0",
        "prettier-plugin-solidity": "^1.0.0-beta.10",
        "solhint": "^3.2.1",
        "solhint-plugin-prettier": "^0.0.5",
        "solidity-coverage": "^0.8.10",
        "ts-node": "^8.5.4",
        "typechain": "^8.3.0",
        "typescript": "^4.3.2"
    },
    "scripts": {
        "compile": "hardhat compile",
        "lint": "eslint . --ext .ts",
        "prettier:check": "prettier-check contracts/**/*.sol test/**/*.ts types/*.ts",
        "size-contracts": "hardhat compile && hardhat size-contracts",
        "test": "hardhat test",
        "clear-cache": "rm -rf artifacts/ cache/ typechain/",
        "coverage": "RUN_COVERAGE=1 hardhat coverage"
    }
}
